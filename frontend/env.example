# Конфигурация API
# Скопируйте этот файл в .env.local и настройте значения

# URL бэкенда (по умолчанию localhost:8000)
NEXT_PUBLIC_API_URL=http://localhost:8000

# Использовать мок-данные (true/false)
# По умолчанию true для разработки
NEXT_PUBLIC_USE_MOCK=true

# Таймаут для API запросов (в миллисекундах)
NEXT_PUBLIC_API_TIMEOUT=30000
